{
	"name": "pr-stacker",
	"version": "0.0.1",
	"private": true,
	"description": "EngineeredDev PR Stack bot for folding stacks of PRs",
	"author": "EngineeredDev",
	"license": "ISC",
	"homepage": "https://github.com/EngineeredDev/pr-stacker",
	"keywords": ["probot", "github", "probot-app"],
	"scripts": {
		"start": "concurrently \"npm run build:watch\" \"npm run probot:watch\"",
		"build": "tsc",
		"build:watch": "tsc --watch",
		"probot:watch": "nodemon --watch lib --ext js --exec \"probot run ./lib/index.js\"",
		"start:prod": "probot run ./lib/index.js"
	},
	"dependencies": {
		"@probot/adapter-aws-lambda-serverless": "^4.0.3",
		"@pulumi/aws": "^6.77.1",
		"@pulumi/pulumi": "^3.163.0",
		"@sentry/aws-serverless": "^9.14.0",
		"esbuild": "^0.25.3",
		"graphology": "^0.26.0",
		"graphology-dag": "^0.4.1",
		"graphology-traversal": "^0.3.1",
		"probot": "^13.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^20.0.0",
		"concurrently": "^9.1.2",
		"graphology-types": "^0.24.8",
		"nock": "^14.0.0-beta.5",
		"nodemon": "^3.1.9",
		"smee-client": "^2.0.0",
		"typescript": "^5.3.3",
		"vitest": "^1.3.1"
	},
	"engines": {
		"node": ">= 18"
	},
	"type": "module"
}
